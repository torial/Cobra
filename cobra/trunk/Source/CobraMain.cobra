class CobraMain

	def main is shared
		startTime = DateTime.now
		try
			try
				CobraCommandLine().run()
			catch ex as Exception
				print '\n[ex.message]\n'
				CobraCore.dumpStack()
				print '\n[ex.message]\n'
				print 'EXITED ON UNCAUGHT EXCEPTION'

		finally
			print 'Duration = [DateTime.now.subtract(startTime)]'
